{
    "overwrite": true,
    "schema": "./schema.graphql",
    "generates": {
        "src/generated/schema.ts": {
            "plugins": [
                "typescript",
                "typescript-resolvers"
            ],
            "config": {
                "skipTypename": true,
                "contextType": "../resolvers#ResolverContext",
                "mappers": {
                    "Company": "../db/types#CompanyEntity",
                    "Job": "../db/types#JobEntity"
                }
            }
        }
    }
}